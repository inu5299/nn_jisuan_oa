<view><swiper indicator-dots="true"><block wx:for="{{imgUrls}}" wx:for-item="img" wx:for-index="key" wx:key="key"><swiper-item><image class="swiper_img" src="{{img}}"></image></swiper-item></block></swiper><view class="title"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="key"><block wx:if="{{item.tab_id==tabID}}"><uni-tag vue-id="{{'1-'+key}}" inverted="{{false}}" circle="{{true}}" text="{{item.name}}" id="{{item.tab_id}}" type="primary" size="small" data-event-opts="{{[['^click',[['onClickTag',['$0'],[[['tabList','',key,'tab_id']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-tag></block><block wx:else><uni-tag vue-id="{{'2-'+key}}" inverted="{{true}}" circle="{{true}}" text="{{item.name}}" id="{{item.tab_id}}" type="primary" size="small" data-event-opts="{{[['^click',[['onClickTag',['$0'],[[['tabList','',key,'tab_id']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-tag></block></block></view><view class="uni-list"><block wx:for="{{showList}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view data-event-opts="{{[['tap',[['clickArticle',[key]]]]]}}" class="uni-media-list" bindtap="__e"><view class="uni-media-list-logo"><block wx:if="{{showImg}}"><image class="cover_image" src="{{value.src}}" mode="aspectFit"></image></block></view><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{value.report_title}}</view><view class="tag"><block wx:for="{{value.tag}}" wx:for-item="tag_name" wx:for-index="__i0__"><view class="tag-view"><uni-tag class="tag_node" vue-id="{{'3-'+key+'-'+__i0__}}" mark="{{true}}" text="{{tag_name}}" type="primary" size="small" bind:__l="__l"></uni-tag></view></block></view></view></view></view></block></view><uni-load-more vue-id="4" status="{{status}}" contentText="{{contentText}}" bind:__l="__l"></uni-load-more><view style="height:150rpx;"></view></view>